# Uncomment this line to define a global platform for your project
platform :ios, '10.0'

target 'MoDb' do

   # Uncomment this line if you're using Swift
   use_frameworks!

   # Pods for MoDB
   pod 'Alamofire'
   pod 'Kingfisher'
   pod 'SwiftyJSON', :git => 'https://github.com/appsailor/SwiftyJSON', :branch => 'swift3'

end

